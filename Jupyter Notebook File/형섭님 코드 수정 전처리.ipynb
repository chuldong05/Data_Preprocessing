    "# \"역명칭\" 컬럼 값이 일치하는 인덱스 검색\n",
    "matching_indices = []\n",
    "for index, row in df2.iterrows():\n",
    "    station_name = row['역명칭']\n",
    "    pattern = r\"\\b{}\\b\".format(station_name.replace(\"(\", r\"\\(\").replace(\")\", r\"\\)\"))\n",
    "    regex = re.compile(pattern)\n",
    "    if any(regex.search(station) for station in Wheelchair_Ramp['역명칭']):\n",
    "        matching_indices.append(index)\n",
    "\n",
    "# 일치하는 인덱스에 대해 \"엘리베이터\" 컬럼에 1 표시\n",
    "df2.loc[matching_indices, '전화번호'] = Tel['전화번호']\n",
    "\n",
    "df2.head(50)"
   ]
